import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        int i,num,sum=0;
        System.out.print("enter the number");
        num = in.nextInt();
        for(i=1;i<=num;i++)
            sum +=i;    
        
        System.out.print(sum);
    }
}
