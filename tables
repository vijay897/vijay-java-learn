import java.util.Scanner;
public class vijay{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        int num,i,sum;
        System.out.print("enter the numbers ");
        num = in.nextInt();
        for(i=1;i<=10;i++){
            sum=num*i;
            System.out.println(num+"*"+i+"="+sum);
        }
    }
}
