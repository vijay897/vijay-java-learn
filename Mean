import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        int n,i,sum=0;
        float mean;
        System.out.print("enter the range od list");
        n = in.nextInt();
        
        int[] arr = new int[n];
        
        System.out.print("enter the numbers ");
        for(i=0;i<n;i++){
            arr[i] = in.nextInt();
            sum = sum+arr[i];
        }
        mean = sum/n;
        System.out.println("Mean "+mean);
    }
}
