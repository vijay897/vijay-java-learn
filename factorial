import java.util.Scanner;
public class vijay{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        int fact=1,i,num;
        System.out.print("enter the numbers ");
        num = in.nextInt();
        for(i=1;i<=num;i++)
        fact *=i;
        System.out.print(num+" number "+fact);
    }
}
