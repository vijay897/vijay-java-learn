import java.util.Scanner;

public class Main
{
   public static void main(String[] args)
   {
      Scanner scan = new Scanner(System.in);
      
      int num;
      System.out.print("Enter a Number: ");
       num = scan.nextInt();
       if(num%2==0)
       System.out.println(num+ " Even Number");
       else
       System.out.println(num+ " Odd Number");
      
   }
}
